; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CitrusModManager"
run/main_scene="res://scenes/manager.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://citrusmodmanagerlogo.png"

[autoload]

Shaker="*res://addons/shaker/src/Shaker.gd"
Globals="*res://scripts/globals.gd"

[display]

window/size/resizable=false

[editor_plugins]

enabled=PackedStringArray("res://addons/SmoothScroll/plugin.cfg", "res://addons/shaker/plugin.cfg")

[gui]

common/snap_controls_to_pixels=false
